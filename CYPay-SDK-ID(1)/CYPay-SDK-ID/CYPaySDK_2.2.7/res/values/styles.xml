<resources xmlns:android="http://schemas.android.com/apk/res/android">
        <style name="com_cypay_paysdk_custom_dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="com_cypay_paysdk_custom_progress_dialog" parent="@style/com_cypay_paysdk_custom_dialog">
        <item name="android:windowBackground">@drawable/com_cypay_paysdk_dialog_white</item>
        <item name="android:windowNoTitle">true</item>
    </style>
    
    <style name="com_mobogenie_paysdk_dialog_payment_channels" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="com_mobogenie_paysdk_dialog_payment_channels_land" parent="com_mobogenie_paysdk_dialog_payment_channels">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>
    
    <!-- 渠道选择界面 -->
    <style name="com_mobogenie_paysdk_channels_style_item_mainlayout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">vertical</item>
        <item name="android:background">@color/transparent</item>
    </style>
    <style name="com_mobogenie_paysdk_channels_style_item_layout">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">65dip</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:background">@color/transparent</item>
    </style>
    <style name="com_mobogenie_paysdk_channels_style_item_layout_land">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:background">@color/transparent</item>
    </style>    
    <style name="com_mobogenie_paysdk_channels_style_item_tv">
        <item name="android:layout_marginLeft">32dip</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">21sp</item>
        <item name="android:textColor">@drawable/com_mobogenie_paysdk_channels_text_xbg</item>
    </style>
    <style name="com_mobogenie_paysdk_channels_style_item_iv">
        <item name="android:layout_marginLeft">28dip</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="com_mobogenie_paysdk_channels_style_item_gv">
        <item name="android:numColumns">3</item>
        <item name="android:horizontalSpacing">1dip</item>
        <item name="android:verticalSpacing">1dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0dip</item>
        <item name="android:listSelector">@color/transparent</item>
    </style>
    <style name="com_mobogenie_paysdk_channels_style_item_line">
         <item name="android:layout_marginLeft">2dip</item>
         <item name="android:layout_marginRight">2dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.5dip</item>
		<item name="android:background">@color/com_cypay_paysdk_channels_line</item>        
    </style>
    
       <style name="com_cypay_paysdk_layout_style_margin">
         <item name="android:layout_marginLeft">@dimen/com_cypay_paysdk_all_layout_margin_left_size</item>
         <item name="android:layout_marginRight">@dimen/com_cypay_paysdk_all_layout_margin_right_size</item>
         <item name="android:layout_marginTop">@dimen/com_cypay_paysdk_all_layout_margin_top_size</item>
         <item name="android:layout_marginBottom">@dimen/com_cypay_paysdk_all_layout_margin_bottom_size</item>      
    </style>


</resources>